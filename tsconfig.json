{
    "compilerOptions": {
      "target": "ES2017",
      "lib": ["ES2017"],
      "module": "CommonJS",
      "strictPropertyInitialization": false,
      "rootDir": "src/",
      "outDir": "dist",
      "declaration": true,
      "composite": true,
      "baseUrl": "./",
      "esModuleInterop": true,
      "strict": true,
      "noImplicitReturns": true,
      "skipLibCheck": true,
      // "noUnusedLocals": true,
      // "noUnusedParameters": true,
      "forceConsistentCasingInFileNames": true,
    },
    "include": ["src"],
  }